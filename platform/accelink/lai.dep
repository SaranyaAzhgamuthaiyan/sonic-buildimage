#DPKG FRK
DPATH       := $($(ACCELINK_LAI)_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) platform/accelink/lai.mk platform/accelink/lai.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
DEP_FILES   += $(shell git ls-files $(DPATH))

$(ACCELINK_LAI)_CACHE_MODE  := GIT_CONTENT_SHA 
$(ACCELINK_LAI)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(ACCELINK_LAI)_DEP_FILES   := $(DEP_FILES)

$(ACCELINK_LAI_DEV)_CACHE_MODE  := GIT_CONTENT_SHA 
$(ACCELINK_LAI_DEV)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(ACCELINK_LAI_DEV)_DEP_FILES   := $(DEP_FILES)

